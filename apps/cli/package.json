{
  "name": "@opentoolbox/cli",
  "description": "cli",
  "version": "0.0.2",
  "author": {
    "name": "Robert Brightline",
    "email": "robert.brightline@gmail.com",
    "url": "https://beemood.github.io"
  },
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "icon": "https://raw.githubusercontent.com/beemood/opentoolbox/refs/heads/main/apps/cli/favicon.png",
  "homepage": "https://beemood.github.io/opentoolbox/cli",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "bin": {
    "toolbox": "./bin/toolbox.mjs"
  },
  "files": [
    "dist",
    "bin",
    "!**/*.tsbuildinfo",
    "generators.json"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "apps/cli/dist",
          "main": "apps/cli/src/index.ts",
          "tsConfig": "apps/cli/tsconfig.lib.json",
          "rootDir": "apps/cli/src",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./apps/cli/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./apps/cli/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      },
      "doc": {
        "command": "npx typedoc --entryPoints {projectRoot}/src/index.ts --out public/{projectRoot} --tsconfig {projectRoot}/tsconfig.lib.json"
      }
    }
  },
  "dependencies": {
    "@nx/devkit": "21.2.1",
    "commander": "^14.0.0",
    "tslib": "^2.3.0"
  },
  "generators": "./generators.json"
}
